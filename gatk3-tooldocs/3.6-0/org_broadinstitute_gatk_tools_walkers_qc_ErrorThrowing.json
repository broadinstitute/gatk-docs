{
  "summary": "A walker that simply throws errors.",
  "parallel": [
    {
      "arg": "-nct",
      "name": "NanoSchedulable",
      "link": "https://www.broadinstitute.org/gatk/guide/tooldocs/org_broadinstitute_gatk_engine_CommandLineGATK.html#-nct"
    },
    {
      "arg": "-nt",
      "name": "TreeReducible",
      "link": "https://www.broadinstitute.org/gatk/guide/tooldocs/org_broadinstitute_gatk_engine_CommandLineGATK.html#-nt"
    }
  ],
  "activeregion": {},
  "partitiontype": "LOCUS",
  "walkertype": "LocusWalker",
  "arguments": [
    {
      "summary": "Java class of exception to throw",
      "name": "--exception",
      "synonyms": "-E",
      "type": "String",
      "required": "yes",
      "fulltext": "",
      "defaultValue": "NA",
      "minValue": "NA",
      "maxValue": "NA",
      "minRecValue": "NA",
      "maxRecValue": "NA",
      "rodTypes": "NA",
      "kind": "required_in",
      "options": []
    },
    {
      "summary": "Determines which method to fail in",
      "name": "--failMethod",
      "synonyms": "-fail",
      "type": "FailMethod",
      "required": "no",
      "fulltext": "",
      "defaultValue": "MAP",
      "minValue": "NA",
      "maxValue": "NA",
      "minRecValue": "NA",
      "maxRecValue": "NA",
      "rodTypes": "NA",
      "kind": "optional_param",
      "options": [
        {
          "summary": "",
          "name": "MAP"
        },
        {
          "summary": "",
          "name": "REDUCE"
        },
        {
          "summary": "",
          "name": "TREE_REDUCE"
        }
      ]
    }
  ],
  "refwindow": {},
  "description": "  Allows us to test that the engine is behaving as expected with error handling",
  "name": "ErrorThrowing",
  "annotinfo": "",
  "readfilters": [
    {
      "filename": "org_broadinstitute_gatk_engine_filters_MalformedReadFilter.html",
      "name": "MalformedReadFilter"
    },
    {
      "filename": "org_broadinstitute_gatk_engine_filters_BadCigarFilter.html",
      "name": "BadCigarFilter"
    },
    {
      "filename": "org_broadinstitute_gatk_engine_filters_UnmappedReadFilter.html",
      "name": "UnmappedReadFilter"
    },
    {
      "filename": "org_broadinstitute_gatk_engine_filters_NotPrimaryAlignmentFilter.html",
      "name": "NotPrimaryAlignmentFilter"
    },
    {
      "filename": "org_broadinstitute_gatk_engine_filters_FailsVendorQualityCheckFilter.html",
      "name": "FailsVendorQualityCheckFilter"
    },
    {
      "filename": "org_broadinstitute_gatk_engine_filters_DuplicateReadFilter.html",
      "name": "DuplicateReadFilter"
    }
  ],
  "downsampling": {
    "by": "BY_SAMPLE",
    "to_cov": 1000
  },
  "group": "Toy Examples (Exclude)",
  "annotfield": ""
